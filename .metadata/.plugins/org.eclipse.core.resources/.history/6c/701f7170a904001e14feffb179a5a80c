package oopWithNLayeredApp.entities;

public class Product {
	private int id;
	private String name;
	private double unitPrice;

	public Product() {
		super();
	}

	public Product(int id, String name, double unitPrice) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
	}

}
